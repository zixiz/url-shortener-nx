{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["development"],
    "baseUrl": ".",
    "paths": {
      "@url-shortener-nx/management-service": ["apps/auth-service/src/main.ts"],
      "@url-shortener-nx/redirect-service": [
        "apps/redirect-service/src/main.ts"
      ],
      "@url-shortener-nx/shared": ["libs/shared/src/index.ts"],
      "@url-shortener-nx/url-service": ["apps/url-service/src/main.ts"],
      "@url-shortener-nx/worker-service": ["apps/worker-service/src/main.ts"]
    }
  }
}
